{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  //"include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "noImplicitAny": true,
    "strict": false,
    "allowJs": true,
    "isolatedModules": true,
    "noImplicitThis": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["**/**"],

  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
